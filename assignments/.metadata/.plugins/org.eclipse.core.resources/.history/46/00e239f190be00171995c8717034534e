package com.revature.BankingSystem.BankClasses;

import java.util.ArrayList;

public class RegularUser extends User{
	private ArrayList<Account> Accounts = new ArrayList<Account>();
	
		
	public RegularUser(int userId, String userName, String firstName, String lastName,
			String password ,ArrayList<Account> accounts) {
		super(userId,userName,firstName,lastName,password);
		Accounts = accounts;
	}
	public void addAccount(Account newAcc) {
		Accounts.add(newAcc);
	}
	public void removeAccount(int i) {
		Accounts.remove(i);
	}
	public Account getAccount(int i) {
		return Accounts.get(i);
	}
}
