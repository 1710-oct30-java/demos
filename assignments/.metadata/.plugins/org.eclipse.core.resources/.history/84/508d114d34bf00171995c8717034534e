package com.revature.BankingSystem.BankDataManagement;

import java.io.FileInputStream;
import java.io.IOException;
import java.io.ObjectInputStream;

public class UserSaving {
	
	public boolean loadUserList(UserList userList) {
		 try {
	         FileInputStream fileIn = new FileInputStream("userList.txt");
	         ObjectInputStream in = new ObjectInputStream(fileIn);
	         userList = (UserList) in.readObject();
	         in.close();
	         fileIn.close();
	         return true;
	      } catch (IOException i) {
	         i.printStackTrace();
	         return false;
	      } catch (ClassNotFoundException c) {
	         System.out.println("UserList class not found");
	         c.printStackTrace();
	         return false;
	      }
	}
	
}
