package com.revature.BankingSystem.BankClasses;

public class Account {
	private int accId;
	private String accType;
	private double accBalence;
	private User accOwner;
	
	
	public Account(int accId, String accType, double accBalence, User accOwner) {
		super();
		this.accId = accId;
		this.accType = accType;
		this.accBalence = accBalence;
		this.accOwner = accOwner;
	}
	
	public int getAccId() {
		return accId;
	}
	public void setAccId(int accId) {
		this.accId = accId;
	}
	public String getAccType() {
		return accType;
	}
	public void setAccType(String accType) {
		this.accType = accType;
	}
	public double getBalence() {
		return accBalence;
	}
	public void setBalence(double balence) {
		this.accBalence = balence;
	}
	
	@Override
	public String toString() {
		return "Account [accId=" + accId + ", accType=" + accType + ", balence=" + balence + "]";
	}
	
}
