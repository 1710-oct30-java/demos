public class BankingLauncher {

	public static void main(String[] args) {
		RegularUser UserA = new RegularUser(1234, "john123", "John", "Doe", "password");
		UserA.addAccount(new Account());
		ArrayList<User> userList = new ArrayList<>();
		userList.add(UserA);
		System.out.println(UserA);
		UserLogin.login(userList);
	}

}
