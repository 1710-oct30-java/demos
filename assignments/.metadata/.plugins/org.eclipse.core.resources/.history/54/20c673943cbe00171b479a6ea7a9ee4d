package com.revature.Homework1.Q15;

public class pairs {
	private int x;
	private int y;
	public pairs() {
		setX(0);
		setY(0);
	}
	pairs(int a,int b){
		setX(a);
		setY(b);
	}
	public int getX() {
		return x;
	}
	public void setY(int y) {
		this.y = y;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getY() {
		return y;
	}
}
